PRODUCT OF LIST

  #include <stdio.h>

int main()
{
    int n;
    scanf("%d", &n);
    int arr[n];
    double product = 1;

    for(int i = 0; i < n; i++) {
        scanf("%d", &arr[i]);
        product *= arr[i];
    }
    
    printf("The Product of list value is %.2lf\n", product);
    return 0;
}
