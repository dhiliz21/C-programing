Delete the vowels

  #include <stdio.h>

int isVowel(char ch) { 
    ch = (ch >= 'A' && ch <= 'Z') ? ch + 32 : ch;
    return ch == 'a' || ch == 'e' || ch == 'i' || ch == 'o' || ch == 'u';
}

int main() {
    char ch;
    while ((ch = getchar()) != '\n' && ch != EOF) {
        if (!isVowel(ch))
            printf("%c", ch);
    }
    printf("\n");
    return 0;
}
