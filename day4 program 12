Toggle Character

  #include <stdio.h>

int main() {
    char ch;
    while ((ch = getchar()) != '\n' && ch != EOF) {
        if (ch >= 'a' && ch <= 'z')
            printf("%c", ch - 32);
        else if (ch >= 'A' && ch <= 'Z')
            printf("%c", ch + 32);
        else
            printf("%c", ch);
    }
    printf("\n");
    return 0;
}
