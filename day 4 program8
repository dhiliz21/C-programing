Rotate String

  #include <stdio.h>
#include <string.h>

int isRotated(char s[], char goal[]) {
    if (strlen(s) != strlen(goal))
        return 0;

    char doubled[201];
    strcpy(doubled, s);
    strcat(doubled, s);

    return strstr(doubled, goal) != NULL;
}

int main() {
    char s[101], goal[101];
    scanf("%s", s);
    scanf("%s", goal);

    if (isRotated(s, goal))
        printf("true\n");
    else
        printf("false\n");

    return 0;
}
